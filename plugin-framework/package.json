{
  "name": "teleport-plugin-framework",
  "version": "8.0.5",
  "description": "Teleport WASM plugin framework",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/node": "~16.11.6",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "assemblyscript": "^0.19.20",
    "eslint": "~8.1.0",
    "eslint-config-prettier": "~8.3.0",
    "prettier": "~2.4.1",
    "rimraf": "~3.0.2",
    "tsutils": "~3.21.0",
    "typescript": "~4.4.4"
  },
  "scripts": {
    "clean": "rimraf build tmp",
    "asbuild:development": "asc assembly/index.ts --target development",
    "asbuild:production": "asc assembly/index.ts --target production",
    "asbuild:test": "asc assembly/index.test.ts --target test",
    "asbuild:dev": "asc dev/index.ts --target dev",
    "asbuild": "npm run asbuild:development && npm run asbuild:production && npm run asbuild:test"
  },
  "author": "Victor Sokolov <gzigzigzeo@evilmartians.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@assemblyscript/loader": "^0.19.20",
    "as-sleep": "^0.0.2",
    "as-uuid": "^0.0.4",
    "tslib": "~2.3.1"
  },
  "volta": {
    "node": "16.13.0"
  }
}
